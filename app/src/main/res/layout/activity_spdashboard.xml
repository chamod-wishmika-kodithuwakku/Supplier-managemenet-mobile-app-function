<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#181F22"
    tools:context=".Spdashboard">


    <Button
        android:id="@+id/add_delivery_note"
        android:layout_width="204dp"
        android:layout_height="60dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="384dp"
        android:backgroundTint="#A1FF9800"
        android:text="Add Delivery Note"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/supplier_management"
        android:layout_width="202dp"
        android:layout_height="63dp"
        android:layout_marginStart="104dp"
        android:layout_marginBottom="184dp"
        android:backgroundTint="#A1FF9800"
        android:text="Supplier Management"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="340dp"
        android:layout_height="263dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sup" />

</androidx.constraintlayout.widget.ConstraintLayout>